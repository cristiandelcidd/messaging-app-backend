{
  "compilerOptions": {
    "target": "es5",
    "lib": [],
    "experimentalDecorators": true,
    "module": "commonjs",
    "rootDir": "./src",
    // "baseUrl": "./",
    // "outFile": "./",
    "outDir": "./dist",
    "removeComments": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  }
}
